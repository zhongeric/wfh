{
  "manifest_version": 2,
  "name": "WFH",
  "description": "Helping you work from home by making your feeds a little less addicting.",
  "author": "Eric Zhong",
  "version": "0.0.0.1",
  "web_accessible_resources": ["script.js"],
  "permissions": ["activeTab", "storage"],
  "background": {
     "scripts": ["background.js"],
     "persistent": false
  },
  "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
          "64": "icons/64x64.png",
          "128": "icons/128x128.png",
          "256": "icons/256x256.png"
        },
        "default_title": "WFH"
  }
}
